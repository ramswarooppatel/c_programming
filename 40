// Write a C program to convert a string to uppercase.
//20
#include <stdio.h>
#include <string.h>
int main() {
    char str[] = "Hello World!";
    for (int i = 0; str[i] != '\0'; i++) {
        if (str[i] >= 'a' && str[i] <= 'z')
            str[i] -= 32;
    }
    printf("%s", str);
    return 0;
}
// RPX~PYCHUNK @ C
