// Category: File Handling
// Question 6: Write a C program to append content to an existing file.
#include <stdio.h>
int main() {
    FILE *file = fopen("file.txt", "a");
    if (file == NULL) {
        printf("Could not open file\n");
        return 1;
    }
    fprintf(file, "\nAppended content!");
    fclose(file);
    return 0;
}
// RPX~PYCHUNK @ C
