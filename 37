// Write a C program to check if a string is a palindrome.
//17
#include <stdio.h>
#include <string.h>
int main() {
    char str[] = "madam";
    int len = strlen(str);
    int flag = 1;
    for (int i = 0; i < len / 2; i++) {
        if (str[i] != str[len - i - 1]) {
            flag = 0;
            break;
        }
    }
    if (flag) printf("Palindrome");
    else printf("Not Palindrome");
    return 0;
}
