// Write a C program to count the number of consonants in a string.
#include <stdio.h>
#include <string.h>
int main() {
    char str[] = "Hello World!";
    int count = 0;
    for (int i = 0; i < strlen(str); i++) {
        if ((str[i] >= 'a' && str[i] <= 'z' || str[i] >= 'A' && str[i] <= 'Z') &&
            !(str[i] == 'a' || str[i] == 'e' || str[i] == 'i' || str[i] == 'o' || str[i] == 'u' ||
              str[i] == 'A' || str[i] == 'E' || str[i] == 'I' || str[i] == 'O' || str[i] == 'U'))
            count++;
    }
    printf("%d", count);
    return 0;
}
// RPX~PYCHUNK @ C
