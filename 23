#include <stdio.h>
//online compiler -rpxx
int main() {
    int n, i, sum = 0;
    printf("Enter the nummber of elements: ");
    scanf("%d", &n);
    int arr[n];
    printf("Enter the elements: ");
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }
    printf("The sum of all elements is %d\n", sum);
    return 0;
}
